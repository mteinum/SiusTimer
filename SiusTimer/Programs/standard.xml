<?xml version="1.0" encoding="UTF-8"?>
<program name="25m STANDARD PISTOL">
  <!-- UI: start (counter) / next group -->
  <group id="PREPARATION">
    <step sound="PREPARATION TIME BEGINS NOW" delay-seconds="150"/>
  </group>
  <group id="SIGHTING">
    <step sound="150 SECONDS - SIGHTING SERIES - LOAD" delay-seconds="60" timer="start"/>
    <step sound="ATTENTION" delay-seconds="wait" light="on"/>
    <step sound="UNLOAD" delay-seconds="60"/>
  </group>
  <group id="150s" repeat="4">
    <step sound="150 SECONDS - LOAD" delay-seconds="60" timer="start"/>
    <step sound="ATTENTION" delay-seconds="wait" light="on"/>
    <step sound="UNLOAD" delay-seconds="60"/>
  </group>
  <group id="20s" repeat="4">
    <step sound="20 SECONDS - LOAD" delay-seconds="60" timer="start"/>
    <step sound="ATTENTION" delay-seconds="wait" light="on"/>
    <step sound="UNLOAD" delay-seconds="60"/>
  </group>
  <group id="10s" repeat="4">
    <step sound="10 SECONDS - LOAD" delay-seconds="60" timer="start"/>
    <step sound="ATTENTION" delay-seconds="wait" light="on"/>
    <step sound="UNLOAD" delay-seconds="60"/>
  </group>
</program>